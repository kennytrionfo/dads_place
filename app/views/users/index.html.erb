<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<h1>User</h1>

<% @users.each do |user| %>
  <h3><%= user.first_name %></h3>
  <h3><%= user.last_name %></h3>
  <h3><%= user.email %></h3>
  <h3><%= user.password %></h3>
  <%= link_to 'Show user', user_path(user.id) %>
<% end %>
